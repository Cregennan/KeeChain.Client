        <ProgressBar Name="Bar3" Minimum="0" Maximum="100" Value="60" Width="340" Height="2" Canvas.Top="374" Canvas.Left="60"></ProgressBar>
        
        <Rectangle IsVisible="True" Canvas.Top="400" Canvas.Left="60" Fill="Transparent" Stroke="Gray" RadiusX="4" RadiusY="4" Width="340" Height="60" StrokeThickness="1"/>
