        <TextBlock Name="Random2" Canvas.Top="255" Canvas.Left="295" FontWeight="Bold" FontSize="25">126632</TextBlock>
        <ProgressBar Name="Bar2" Minimum="0" Maximum="100" Value="60" Width="340" Height="2" Canvas.Top="294" Canvas.Left="60"></ProgressBar>
        
        <Rectangle Canvas.Top="320" Canvas.Left="60" Fill="Transparent" Stroke="Gray" RadiusX="4" RadiusY="4" Width="340" Height="60" StrokeThickness="1"/>
