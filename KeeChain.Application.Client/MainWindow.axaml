<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="800" Height="670"
        x:Class="KeeChain.Application.Client.MainWindow"
        Title="MainWindow">
    <Canvas Name="MainCanvas">
        <TextBlock FontSize="30" FontWeight="Bold" Canvas.Top="70" Canvas.Left="60">Клиент KeeChain</TextBlock>
        <Image Canvas.Left="495" Canvas.Top="100" Width="250" Height="250" Source="/Assets/secure.png"></Image>
        <Label Name="KeysLabel" Canvas.Top="120" Canvas.Left="60">Сохраненные ключи</Label>
        
        <Button Name="AddBtn" Canvas.Top="590" Canvas.Left="60">Добавить</Button>
        <Button Name="RemoveBtn" Canvas.Top="590" Canvas.Left="170">Удалить</Button>
    </Canvas>
</Window>
