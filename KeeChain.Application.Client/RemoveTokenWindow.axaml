        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="450" Height="200"
        x:Class="KeeChain.Application.Client.RemoveTokenWindow"
        Title="RemoveTokenWindow">
